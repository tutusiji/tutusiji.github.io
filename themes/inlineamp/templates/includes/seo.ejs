<meta name="author" content="<%= site.customConfig.avatar %>">

<meta itemprop="name" content="<%= siteTitle %>">
<meta itemprop="description" content="<%- site.customConfig.metaDescription || themeConfig.siteDescription %>">
<meta itemprop="image" content="<%= site.customConfig.siteImage %>">

<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="<%= siteTitle %>">
<meta name="twitter:description" content="<%- site.customConfig.metaDescription || themeConfig.siteDescription %>">
<meta name="twitter:image" content="<%= site.customConfig.siteImage %>" />
<meta name="twitter:image:alt" content="<%= siteTitle %>" />


<meta property="og:type" content="website">
<meta property="og:url" content="<%= themeConfig.domain %>">
<meta property="og:title" content="<%= siteTitle %>">
<meta property="og:description" content="<%- site.customConfig.metaDescription || themeConfig.siteDescription %>">
<meta property="og:image" content="<%= site.customConfig.siteImage %>">
<meta property="og:image:alt" content="<%= siteTitle %>" />
<meta property="og:site_name" content="<%= siteTitle %>">
<meta property="og:locale" content="zh_CN">
<meta property="tag" content="<%= siteTitle %>" />
<% if (site.customConfig.ldjson) { %>

<script type="application/ld+json">
  {
    "@context":"https://schema.org",
    "@type":"BreadcrumbList",
    "itemListElement":
    [
     {
      "@type":"ListItem",
      "position":1,
      "name":"Home",
      "item":"<%= themeConfig.domain %>"
     }
     ]
    }

</script>
<% } %>
<% if (site.customConfig.ldjsonlogo) { %>

  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Organization",
      "name": "<%= themeConfig.siteName %>",
      "url": "<%= themeConfig.domain %>",
      "logo": "<%= site.customConfig.ldjsonlogos %>"
    }
    </script>
    <% } %>